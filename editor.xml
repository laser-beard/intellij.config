<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Monospaced" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Python" value="true" />
      </map>
    </option>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo)\b.*" />
    <pattern pattern="\b(fixme)\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\b(del)\b.*">
      <option name="FOREGROUND" value="ff5954" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff857d" />
    </pattern>
  </component>
</application>